{
	"$schema": "http://json.schemastore.org/prettierrc",
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"jsxSingleQuote": true,
	"printWidth": 90,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"quoteProps": "as-needed",
	"useTabs": true
}
